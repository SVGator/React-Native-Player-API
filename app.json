{
  "expo": {
    "name": "SVGator's Player API React Native Demo",
    "slug": "svgators-player-api-react-native-demo",
    "scheme": "svgatorsplayerapireactnativedemo",
    "version": "2.0.1",
    "orientation": "portrait",
    "platforms": ["ios", "android", "web"],
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bundleIdentifier": "com.svgator.playerapireactnativedemo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "newArchEnabled": true,
      "package": "com.svgator.playerapireactnativedemo"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
